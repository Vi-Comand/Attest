                                    </select>
                                </td>
                            </tr>
                            <tr>
                                <td>Категория</td>
                                <td>

                                    <select id="kategor" asp-for="Zayavlen.kategor" required>

                                        <option value="" selected="selected">Выберите категорию</option>
                                        <option value="Первая">Первая</option>
                                        <option value="Высшая">Высшая</option>
                                    </select>

                                </td>
                            </tr>
                            <tr>
                                <td>Дата последней аттестации</td>
                                <td>


                                    <input type="text" asp-for="Zayavlen.data_last_att" id="date" name="date" placeholder="Дата" required>
                                    <script>

                                        $(function () {
                                            $('#date').daterangepicker({
                                                singleDatePicker: true,
                                                locale: {
                                                    format: 'DD.MM.YYYY',
                                                    "firstDay": 1
                                                }
                                            });
                                        });
                                    </script>
                                </td>
                            </tr>
                            <tr>
                                <td>Категория работника</td>
                                <td>
                                    <input type="text" asp-for="Zayavlen.kategor_rabot" />
                                </td>
                            </tr>
                            <tr>
                                <td>Дата подачи заявления</td>
                                <td>
                                    <input type="text" asp-for="Zayavlen.data_podachi" disabled />
                                </td>
                            </tr>
                            <tr>
                                <td>Дата обновления заявления</td>
                                <td>
                                    <input type="text" asp-for="Zayavlen.data_obnovl" disabled />
                                </td>
                            </tr>
                            <tr>
                                <td>Статус</td>
                                <td>
                                    <input type="text" asp-for="Zayavlen.status" disabled />
                                </td>
                            </tr>
                            <tr>
                                <td>Итоговый балл</td>
                                <td>
                                    <input type="text" asp-for="Zayavlen.ball" disabled />
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <!-- Button trigger modal -->
            <button class="btn-lg" data-toggle="modal" data-target="#obr">
                Добавить образование
            </button>



            @foreach (var row in ViewBag.Obr)
            {
                @if (row.tip_obr == 1)
                {
                    <div class="panel panel-default">
                        <div class="panel-heading" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                            <h4 class="panel-title">
                                <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                                    Образование
                                </a>
                            </h4>
                        </div>
                        <div id="collapseTwo" class="panel-collapse collapse">
                            <div class="panel-body">

                                <table>
                                    <tr>
                                        <td>ОО</td>
                                        <td>
                                            <input type="text" value="@row.oo" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Период обучения</td>
                                        <td>
                                            <input type="text" value="@row.period" />


                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Муниципалитет</td>
                                        <td>
                                            <input type="text" value="@row.mo" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Специальность</td>
                                        <td>
                                            <input type="text" value="@row.special" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Квалификация</td>
                                        <td>
                                            <input type="text" value="@row.kvalif" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Тип документа</td>
                                        <td>
                                            <input type="text" value="@row.nazv_doc" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Серия</td>
                                        <td>
                                            <input type="text" value="@row.ser_doc" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Номер документа</td>
                                        <td>
                                            <input type="text" value="@row.nom_doc" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Дата выдачи</td>
                                        <td>
                                            <input type="text" id="d_v_o" value="@row.data_doc" />
                                            <script>
                                                $(function () {
                                                    $('#d_v_o').daterangepicker({
                                                        singleDatePicker: true,
                                                        locale: {
                                                            format: 'DD.MM.YYYY',
                                                            "firstDay": 1
                                                        }
                                                    });
                                                });
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Регистрационный номер</td>
                                        <td>
                                            <input type="text" value="@row.reg_nom" />
                                        </td>
                                    </tr>

                                </table>
                            </div>
                        </div>
                    </div>
                }

                @if (row.tip_obr == 2)
                {
