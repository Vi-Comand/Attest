                                <table>
                                    <tr>
                                        <td>Организация</td>
                                        <td>
                                            <input type="text" value="@row.oo" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Период обучения</td>
                                        <td>

                                            <input type="text" name="daterange" value="@row.period" />


                                            <script>

                                                $(function () {
                                                    $('input[name="daterange"]').daterangepicker({
                                                        opens: 'left',
                                                        locale: {
                                                            "format": "DD.MM.YYYY",
                                                            "autoApply": true,
                                                            "applyLabel": "Выбрать",
                                                            "cancelLabel": "Отменить",
                                                            "firstDay": 1
                                                        }
                                                    });
                                                });
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Место прохождения</td>
                                        <td>
                                            <input type="text" value="@row.mo" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Кол-во часов</td>
                                        <td>
                                            <input type="text" value="@row.kol_chas" />
                                        </td>
                                    </tr>
                                    <tr>
