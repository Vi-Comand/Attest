                                        <option value="Кущевский ">Кущевский</option>
                                        <option value="Лабинский ">Лабинский</option>
                                        <option value="Ленинградский ">Ленинградский</option>
                                        <option value="Мостовский ">Мостовский</option>
                                        <option value="Новокубанский ">Новокубанский</option>
                                        <option value="Новопокровский ">Новопокровский</option>
                                        <option value="Отрадненский ">Отрадненский</option>
                                        <option value="Павловский ">Павловский</option>
                                        <option value="Приморско-Ахтарский ">Приморско-Ахтарский</option>
                                        <option value="Северский ">Северский</option>
