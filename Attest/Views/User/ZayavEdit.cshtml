                                            </select>
                                            <script>
                                                $("#kvalif_@row.Id").val("@row.kvalif").change()
                                            </script>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Название документа</td>
                                        <td>
                                            <input type="text" value="@row.nazv_doc" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Серия</td>
                                        <td>
                                            <input type="text" value="@row.ser_doc" />
                                        </td>
