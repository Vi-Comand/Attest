@model Attest.Models.Zayavlen

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Lk-user</title>
</head>
<body>
    <form method="post" asp-controller="User" asp-action="zayav">
       
        <table>
            @foreach (var row in ViewBag.LK)
            {
            <tr>
                <td>@row.Id</td>
                <td>@row.dolgnost_att</td>
                <td>@row.data_podachi</td>
                <td>@row.data_obnovl</td>
                <td>@row.status</td>
                <td>@row.ball</td>
                <td><button asp-controller="User" asp-action="Zayav" asp-route-id="@row.Id">Открыть</button></td>

                <td> <a asp-controller="User" asp-action="view" asp-route-id="@row.Id">View</a> |<a asp-controller="User" asp-action="edit" asp-route-id="@row.Id">Edit</a> | <a asp-controller="User" asp-action="Delete" asp-route-id="@row.Id" onclick="return confirm('Ты Серьезно ?')">Delete</a></td>
            </tr>
            }
        </table>

    </form>
</body>
</html>